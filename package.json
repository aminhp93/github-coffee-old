  },
  "scripts": {
    "dev": "GENERATE_SOURCEMAP=false craco start",
    "start": "GENERATE_SOURCEMAP=false craco start",
    "build": "craco test & GENERATE_SOURCEMAP=false craco build ",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "test": "CI=true craco test",
    "coverage": "craco test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
      "react-scripts test --bail --watchAll=false --findRelatedTests --passWithNoTests"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3"
  }
}
