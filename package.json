    "axios": "^0.27.2",
    "craco-less": "^2.0.0",
    "dotenv": "^16.0.1",
    "echarts": "^5.3.3",
    "flexlayout-react": "^0.7.4",
    "html-react-parser": "^3.0.0",
    "immutability-helper": "^3.1.1",
    "lexical": "^0.9.1",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
